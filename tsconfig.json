{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "importHelpers": true,
        "noImplicitReturns": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "moduleResolution": "node",
        "typeRoots": [
            "node_modules/@types"
        ],
        "lib": [
            "es2016",
            "dom"
        ],
        "sourceMap": true,
        "allowJs": true
    }
}
